<?xml version='1.0' encoding='utf-8'?>
<widget id="com.espritsolutions.multidominoes" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Keyboard">
        <param name="android-package" value="com.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Toast">
        <param name="android-package" value="nl.xservices.plugins.Toast" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <name>MultiDominoes</name>
    <description>
        Play Dominoes with your friends through your Smart TV
    </description>
    <author email="info@esprit-solutions.com" href="http://esprit-solutions.com">
        eSprit Solutions Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
